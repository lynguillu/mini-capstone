<h1>Edit Camara</h1>
<%= form_tag "/camaras/#{@camara.id}", method: :post do %>

<div class="form-group">
  Name: <input type="text" class="form-control" name="form_name">
</div>
<div class="form-group">
  Model: <input type="text" class="form-control" name="form_model">
</div>
<div class="form-group">
  Mark: <input type="text" class="form-control" name="form_mark">
</div>
<div class="form-group">
  Price: <input type="text" class="form-control" name="form_price">
</div>
<input type="submit" class="btn btn-default" value="create camara">
<% end %>